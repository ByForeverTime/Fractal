<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.redknot.fractalandroid">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.redknot.activity.LogoActivity"></activity>
        <activity android:name="com.redknot.activity.MainActivity"></activity>
        <activity android:name="com.redknot.activity.ListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.redknot.activity.SettingActivity"></activity>
        <activity android:name="com.redknot.setting.Tree2Setting"></activity>
        <activity android:name=".FractalActivity"></activity>
        <activity android:name=".CustomJuliaActivity"></activity>
        <activity android:name="com.redknot.setting.JuliaSetting"></activity>

    </application>

</manifest>
